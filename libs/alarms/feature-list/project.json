{
  "name": "alarms-feature-list",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/alarms/feature-list/src",
  "prefix": "alarms",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "libs/alarms/feature-list/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/alarms/feature-list/**/*.ts",
          "libs/alarms/feature-list/**/*.html"
        ]
      }
    }
  },
  "tags": [
    "domain:alarms",
    "type:feature"
  ]
}
